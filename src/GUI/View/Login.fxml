<AnchorPane id="AnchorPane" prefHeight="189.0" prefWidth="236.0" styleClass="darkness" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.LoginController">
