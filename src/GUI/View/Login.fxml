      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="4.0" prefHeight="25.0" prefWidth="236.0" styleClass="color2" text="Username:" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
