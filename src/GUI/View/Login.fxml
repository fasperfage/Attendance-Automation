      <TextField fx:id="user" layoutY="33.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />

      <PasswordField fx:id="password" layoutY="102.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
